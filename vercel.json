{
  "rewrites": [
    {
      "source": "/((?!api/.*).*)",
      "destination": "/index.html"
    },
    {
      "source": "/api/:match*",
      "destination": "https://nextjsintro-flame.vercel.app/api/:match*"
    },
    {
      "source": "/mockData/product.json",
      "destination": "/public/mockData/product.json"
    },
    {
      "source": "/mockData/productList.json",
      "destination": "/public/mockData/productList.json"
    },
    {
      "source": "/public/mockData/product.json",
      "destination": "/mockData/product.json"
    },
    {
      "source": "/public/mockData/productList.json",
      "destination": "/mockData/productList.json"
    }
  ]
}
