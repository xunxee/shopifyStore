{
  "routes": [
    {
      "src": "/mockData/(product|productList)\\.json$",
      "dest": "/public/mockData/:1.json"
    },
    {
      "src": "/((?!api/.*).*)",
      "dest": "/index.html"
    },
    {
      "src": "/api/:match*",
      "dest": "https://nextjsintro-flame.vercel.app/api/:match*"
    }
  ]
}
